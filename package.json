{
  "name": "vue-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "prod": "vue-cli-service build --mode production",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "fix": "npx eslint -c ./.eslintrc.js src --ext .js,.jsx,.json,.ts,.tsx,.vue --fix",
    "style": "stylelint \"src/**/*.scss\" --cache --cache-location .cache/.stylelintcache --fix",
    "prehusky": "rm -rf .husky && husky install",
    "husky": "npx husky add .husky/pre-push 'ng test --watch=false --code-coverage --browsers=ChromeHeadless' && npx husky add .husky/commit-msg 'npx commitlint --edit $1'",
    "postinstall": "npm run husky"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "bootstrap": "^5.2.0",
    "bootstrap-icons": "^1.9.1",
    "chart.js": "^3.9.1",
    "core-js": "^3.8.3",
    "vue": "^3.2.13",
    "vue-chart-3": "^3.1.8",
    "vue-class-component": "^8.0.0-0",
    "vue-router": "^4.0.3",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/bootstrap": "^5.2.1",
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-e2e-cypress": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-plugin-unit-jest": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/test-utils": "^2.0.0-0",
    "@vue/vue3-jest": "^27.0.0-alpha.1",
    "babel-jest": "^27.0.6",
    "cypress": "^9.7.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "husky": "^8.0.1",
    "jest": "^27.0.5",
    "jsonc-eslint-parser": "^2.1.0",
    "lint-staged": "^11.1.2",
    "naive-ui": "^2.32.1",
    "postcss-html": "^1.5.0",
    "prettier": "^2.4.1",
    "sass": "^1.32.7",
    "sass-loader": "^12.0.0",
    "stylelint": "^14.8.5",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-no-unsupported-browser-features": "^5.0.3",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.2.0",
    "ts-jest": "^27.0.4",
    "typescript": "~4.5.5",
    "vfonts": "0.0.3"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx,json}": [
      "npx eslint -c ./.eslintrc.js src --ext .js,.jsx,.json,.ts,.tsx,.vue --fix",
      "vue-cli-service lint",
      "git add"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  }
}
